{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "noImplicitAny": true,
    "incremental": true,
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "newLine": "lf",
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": true,
    "baseUrl": "./",
  }
}
